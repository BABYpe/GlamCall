{
  "name": "vite-react-typescript-starter",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "build:prod": "vite build --mode production",
    "build:analyze": "npm run build:prod && npx vite-bundle-analyzer dist/stats.html",
    "build:gzip": "npm run build:prod && gzip-size dist/assets/js/*.js",
    "optimize": "npm run build:prod && npm run compress:images && npm run minify:html",
    "compress:images": "imagemin dist/assets/img/* --out-dir=dist/assets/img --plugin=imagemin-mozjpeg --plugin=imagemin-pngquant",
    "minify:html": "html-minifier --input-dir dist --output-dir dist --file-ext html --remove-comments --collapse-whitespace --minify-css --minify-js",
    "preview": "vite preview",
    "lint": "eslint .",
    "deploy": "npm run optimize && npm run preview",
    "deploy:netlify": "npm run optimize && netlify deploy --prod --dir=dist",
    "deploy:vercel": "npm run optimize && vercel --prod",
    "type-check": "tsc --noEmit",
    "test": "echo 'No tests specified' && exit 0",
    "size-check": "bundlesize",
    "lighthouse": "lighthouse http://localhost:4173 --output=html --output-path=./lighthouse-report.html"
  },
  "dependencies": {
    "@hookform/resolvers": "^5.2.1",
    "@supabase/supabase-js": "^2.53.0",
    "bcryptjs": "^3.0.2",
    "date-fns": "^4.1.0",
    "i18next": "^23.7.6",
    "i18next-browser-languagedetector": "^7.2.0",
    "jsonwebtoken": "^9.0.2",
    "lucide-react": "^0.344.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-helmet-async": "^2.0.4",
    "react-hook-form": "^7.61.1",
    "react-hot-toast": "^2.5.2",
    "react-i18next": "^13.5.0",
    "react-query": "^3.39.3",
    "uuid": "^11.1.0",
    "web-vitals": "^5.1.0",
    "yup": "^1.6.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.9.1",
    "@types/react": "^18.3.5",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react": "^4.7.0",
    "autoprefixer": "^10.4.18",
    "bundlesize": "^0.18.2",
    "compression-webpack-plugin": "^11.0.0",
    "eslint": "^9.9.1",
    "eslint-plugin-react-hooks": "^5.1.0-rc.0",
    "eslint-plugin-react-refresh": "^0.4.11",
    "globals": "^15.9.0",
    "gzip-size-cli": "^5.1.0",
    "html-minifier": "^4.0.0",
    "imagemin": "^8.0.1",
    "imagemin-mozjpeg": "^10.0.0",
    "imagemin-pngquant": "^9.0.2",
    "lighthouse": "^11.4.0",
    "netlify-cli": "^17.10.1",
    "postcss": "^8.4.35",
    "rollup-plugin-visualizer": "^5.12.0",
    "tailwindcss": "^3.4.1",
    "terser": "^5.43.1",
    "typescript": "^5.5.3",
    "typescript-eslint": "^8.3.0",
    "vite": "^7.0.6",
    "vite-bundle-analyzer": "^0.7.0",
    "vite-plugin-compression": "^0.5.1",
    "vite-plugin-pwa": "^1.0.2",
    "workbox-window": "^7.0.0"
  }
}
